{
  "public": ".",
  "rewrites": [
    {
      "source": "**",
      "destination": "/alunos.html"
    }
  ],
  "headers": [
    {
      "source": "**/*.@(js|css|html)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000"
        }
      ]
    }
  ]
}