{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/alunos.html",
      "dest": "/alunos.html"
    },
    {
      "src": "/docentes.html",
      "dest": "/docentes.html"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}