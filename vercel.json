{
  "version": 2,
  "functions": {
    "api/index.js": {
      "includeFiles": "public/**"
    }
  },
  "routes": [
    {
      "src": "\/css\/(.*)",
      "dest": "\/css\/$1"
    },
    {
      "src": "\/js\/(.*)",
      "dest": "\/js\/$1"
    },
    {
      "src": "\/img\/(.*)",
      "dest": "\/img\/$1"
    },
    {
      "src": "\/(.*)",
      "dest": "\/api/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}